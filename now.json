{
  "builds": [
    { "src": "*.php", "use": "now-php@canary" },
    { "src": "api/*.php", "use": "now-php@canary" },
    { "src": "api/hitokoto/*.php", "use": "now-php@canary" },
    { "src": "api/img/bing/*.php", "use": "now-php@canary" },
    { "src": "api/user/*.php", "use": "now-php@canary" }
  ],
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404.php" }
  ]
}
